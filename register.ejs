<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Register</title>
    <link rel="stylesheet" href="/css/register.css">
</head>
<body>
    <%- include ('partials/header')%>
    <div class="wrapper-register">
		<div class="from-box-register">
			<h1>Register</h1>
			<div class="input-register">
				<ul class='input-in4'>
					<br>
					<li>Name: <input type="text" name="name" required></li>
					<br>
					<li>Contact Number: <input type="text" name="number" required></li>
					<br>
					<li>Email: <input type="text" name="email" required></li>
					<br>
					<li>Password: <input type="password" name="psw" required></li>
					<br>
					<li>Repeat Password: <input type="password" name="psw-repeat" required></li>
				</ul>
				<br>
				<button type="submit" class="registerbutton">Register</button>

				<p>By creating an account you agree to our <br>
					<a href="#">Terms & Privacy</a>.
				</p>

			</div>
		</div>
    </div>
	<br>
    <%- include ('partials/footer')%>
